{
  "name": "wippets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write **/*.{js,jsx,ts,tsx,css,html,json,md,yml}",
    "prepare": "husky install",
    "test:unit": "vitest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@hookform/resolvers": "2.9.11",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.5",
    "styled-jsx": "5.1.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "1.31.2",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.15.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "@vitejs/plugin-react-swc": "3.2.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.27.5",
    "happy-dom": "8.9.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "typescript": "5.0.0-beta",
    "vitest": "0.29.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,css,html,json,md,yml}": "prettier --write"
  }
}
