{
  "name": "wippets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write **/*.{js,jsx,ts,tsx,css,html,json,md,yml}",
    "prepare": "husky install",
    "test:unit": "vitest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "material-symbols": "0.4.4",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-jsx": "5.1.2"
  },
  "devDependencies": {
    "@playwright/test": "1.30.0",
    "@testing-library/react": "13.4.0",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vitejs/plugin-react-swc": "3.1.0",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "happy-dom": "8.3.2",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "typescript": "5.0.0-beta",
    "vitest": "0.28.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,css,html,json,md,yml}": "prettier --write"
  }
}
